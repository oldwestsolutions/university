{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/login",
      "dest": "/login"
    },
    {
      "src": "/university",
      "dest": "/university"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
} 