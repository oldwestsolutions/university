{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/login",
      "dest": "/login",
      "continue": false
    },
    {
      "src": "/university",
      "dest": "/university",
      "continue": false
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1",
      "continue": false
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        }
      ]
    }
  ]
} 